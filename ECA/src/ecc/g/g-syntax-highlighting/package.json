{
    "name": "g-syntax-highlighting",
    "displayName": "G Syntax Highlighting",
    "description": "G Syntax Highlighting",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "g",
            "aliases": ["g", "g"],
            "extensions": [".g",".gl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "g",
            "scopeName": "source.g",
            "path": "./syntaxes/g.tmLanguage.json"
        }]
    }
}